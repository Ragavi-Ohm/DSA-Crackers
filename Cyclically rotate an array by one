class Compute {
    
    public void rotate(int arr[], int n)
    {
        ArrayList<Integer> a=new ArrayList<>();
        for(int i=0;i<n;i++)
        {
            a.add(arr[i]);
        }
        
        Collections.rotate(a,1);
        for(int i=0;i<n;i++)
        {
            arr[i]=a.get(i);
        }
        
    }
}
